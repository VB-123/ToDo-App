{
    "version": 2,
    "builds": [
       {
         "src": "package.json",
         "use": "@vercel/node"
       }
    ],
    "routes": [
       {
         "src": "/api/(.*)",
         "dest": "/api/$1"
       },
       {
         "src": "/api/(.*)",
         "dest": "/$1"
       }
    ]
    {
 "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
 ]
}

   }
   
